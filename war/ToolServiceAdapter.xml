<?xml version="1.0" encoding="UTF-8"?>
<appvet:ToolServiceAdapter xmlns:appvet="http://csrc.nist.gov/groups/SNS/appvet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://csrc.nist.gov/groups/SNS/appvet ToolServiceAdapter.xsd ">
  <appvet:Description>
    <appvet:Name>appvet:Name</appvet:Name>
    <appvet:Id>name</appvet:Id>
    <appvet:VendorName>appvet:VendorName</appvet:VendorName>
    <appvet:VendorWebsite>http://tempuri.org</appvet:VendorWebsite>
    <appvet:ReportFile>TXT</appvet:ReportFile>
  </appvet:Description>
  <appvet:Protocol>
    <appvet:Type>SYNCHRONOUS</appvet:Type>
    <appvet:Synchronous>
      <appvet:Request>
        <appvet:URL>http://tempuri.org</appvet:URL>
        <appvet:Method>POST</appvet:Method>
        <appvet:Parameter>
          <appvet:Name>appvet:Name</appvet:Name>
          <appvet:Value>DEFINED_AT_RUNTIME</appvet:Value>
        </appvet:Parameter>
      </appvet:Request>
      <appvet:Response>
        <appvet:AppVetRiskHeaderName>APPVET_TOOL_RESULT</appvet:AppVetRiskHeaderName>
        <appvet:ReportPayload>true</appvet:ReportPayload>
      </appvet:Response>
    </appvet:Synchronous>
  </appvet:Protocol>
</appvet:ToolServiceAdapter>
